<form method="post" action="{{ path('app_eventcalendar_delete', {'id': eventcalendar.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ eventcalendar.id) }}">
    <button class="btn">Delete</button>
</form>
